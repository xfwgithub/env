# Python环境变量示例文件
# 复制此文件到.env并根据需要调整

# Python设置
PYTHONPATH=${PYTHONPATH}:${PWD}
PYTHONDONTWRITEBYTECODE=1  # 防止生成__pycache__
PYTHONUNBUFFERED=1         # 立即输出日志

# Poetry配置
POETRY_VIRTUALENVS_IN_PROJECT=true  # 在项目目录内创建虚拟环境

# 开发环境配置
DEBUG=true
LOG_LEVEL=debug

# 项目特定配置
# PROJECT_SPECIFIC_SETTING=value 